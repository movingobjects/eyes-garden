{
  "name": "eyes-garden",
  "productName": "Eyes Garden",
  "version": "0.1.2",
  "description": "Projection for Four Eyes",
  "author": "Scott Thiessen (http://varyd.io)",
  "license": "ISC",
  "homepage": "https://github.com/sccottt/eyes-garden#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sccottt/eyes-garden.git"
  },
  "bugs": {
    "url": "https://github.com/sccottt/eyes-garden/issues"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "app/main.js",
  "scripts": {
    "build:web": "node ./scripts/build.js -b",
    "build": "node ./scripts/build.js",
    "clean": "node ./scripts/clean -spm",
    "clear-packages": "node ./scripts/clean.js -p",
    "dev": "node ./scripts/build.js -w",
    "dev:web": "node ./scripts/build.js -wb",
    "enable-scripts": "node ./scripts/clean -s",
    "package": "node ./scripts/package.js",
    "package:win": "node ./scripts/package.js -w",
    "reinstall": "node ./scripts/clean.js -m",
    "start": "node ./scripts/start.js"
  },
  "config": {
    "package": {
      "appIcon": {
        "icns": "app/resources/app-icon.icns",
        "ico": "app/resources/app-icon.ico"
      },
      "arch": "x64",
      "pathOutput": "app/packages",
      "pathWine": "/Applications/Wine Stable.app",
      "zip": false
    },
    "pathBuild": "app/build"
  },
  "devDependencies": {
    "ajv": "^6.1.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.9",
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "electron-reload": "^1.2.2",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.5",
    "log-box": "^0.4.3",
    "minimist": "^1.2.0",
    "pixi-tween": "^0.2.0",
    "pixi.js": "^4.7.0",
    "varyd-utils": "^1.0.0"
  },
  "log-box": {
    "default": {
      "style": "round",
      "color": "#9f0",
      "textColor": "#9f0",
      "margin": {
        "top": 1,
        "bottom": 2
      }
    }
  }
}
